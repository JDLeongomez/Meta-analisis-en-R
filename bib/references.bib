@article{copasMetaanalysisFunnelPlots2000,
  title = {Meta-Analysis, Funnel Plots and Sensitivity Analysis},
  author = {Copas, John and Shi, Jian Qing},
  year = {2000},
  month = sep,
  journal = {Biostatistics},
  volume = {1},
  number = {3},
  pages = {247-262},
  issn = {1465-4644},
  doi = {10.1093/biostatistics/1.3.247},
  abstract = {Publication bias is a major problem, perhaps the major problem, in meta-analysis (or systematic reviews). Small studies are more likely to be published if their results are `significant' than if their results are negative or inconclusive, and so the studies available for review are biased in favour of those with positive outcomes. Correcting for this bias is not possible without making untestable assumptions. In this paper, a sensitivity analysis is suggested which is based on fitting a model to the funnel plot. Some examples are discussed.*To whom correspondence should be addressed. jbc@stats.warwick.ac.uk},
  file = {D\:\\GDrive\\Zotero\\Biostatistics\\Copas,Shi_2000_Meta-analysis, funnel plots and sensitivity analysis.pdf;D\:\\Zotero Library\\storage\\4L3XIZIS\\260794.html}
}

